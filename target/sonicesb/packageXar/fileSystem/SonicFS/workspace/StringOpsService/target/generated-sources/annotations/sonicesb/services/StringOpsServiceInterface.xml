<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<interface xmlns="http://www.sonicsw.com/sonicxq" name="StringOpsService">
<documentation>StringOpsService POJO service</documentation>
<operation name="concatenate">
<documentation>Concatenate strings</documentation>
<input name="Input">
<parameter xmlns:xs="http://www.w3.org/2001/XMLSchema" collection="true" contentType="text/plain" name="strList" type="xsd:string"/>
<parameter xmlns:xs="http://www.w3.org/2001/XMLSchema" collection="false" contentType="text/plain" name="delimiter" type="xsd:string"/>
</input>
<output name="Output">
<parameter xmlns:xs="http://www.w3.org/2001/XMLSchema" collection="false" contentType="text/plain" name="output" type="xsd:string"/>
</output>
</operation>
<operation name="split">
<documentation>Split string</documentation>
<input name="Input">
<parameter xmlns:xs="http://www.w3.org/2001/XMLSchema" collection="false" contentType="text/plain" name="str" type="xsd:string"/>
<parameter xmlns:xs="http://www.w3.org/2001/XMLSchema" collection="false" contentType="text/plain" name="delimiter" type="xsd:string"/>
</input>
<output name="Output">
<parameter xmlns:xs="http://www.w3.org/2001/XMLSchema" collection="true" contentType="text/plain" name="output" type="xsd:string"/>
</output>
</operation>
<operation name="convert">
<documentation>Convert Fahrenheit To Celcius</documentation>
<input name="Input">
<parameter xmlns:xs="http://www.w3.org/2001/XMLSchema" collection="false" contentType="text/xml" name="fahrenheit" type="xsd:float"/>
</input>
<output name="Output">
<parameter xmlns:xs="http://www.w3.org/2001/XMLSchema" collection="false" contentType="text/plain" name="output" type="xsd:float"/>
</output>
</operation>
</interface>
